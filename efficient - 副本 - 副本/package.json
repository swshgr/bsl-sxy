		"core-js": "^3.8.3",
		"echarts": "^5.3.3",
		"element-ui": "^2.15.9",
		"express": "^4.18.1",
		"husky": "^8.0.1",
		"js-cookie": "^3.0.1",
		"mockjs": "^1.1.0",
		"nodemon": "^2.0.19",
		"vue": "^2.6.14",
		"vue-router": "^3.5.1",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/eslint-parser": "^7.12.16",
		"@vue/cli-plugin-babel": "~5.0.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/cli-plugin-router": "~5.0.0",
		"@vue/cli-plugin-vuex": "~5.0.0",
